&lt;ScriptConfig&gt;'this script creates a CSV file containing address information to be cleansed by an address cleansing application
Option Explicit
Dim MyMeta, x, MyCSVLine, FSO, ft
Dim myPath, myFile

Set MyMeta = CreateObject(&amp;quot;MetaDataLib.MetaFile&amp;quot;)
MyMeta.LoadFromFile(Watch.GetMetadataFileName)

'set file path &amp;amp; name
myPath = Trim(Watch.GetVariable(&amp;quot;global.baseDir&amp;quot;)) &amp;amp; &amp;quot;\2 Processing\&amp;quot;
myFile =  Trim(Watch.GetVariable(&amp;quot;newFileName&amp;quot;))
myFile = myFile &amp;amp; &amp;quot;_unsorted.txt&amp;quot;

'create CSV
Set FSO = CreateObject(&amp;quot;Scripting.FileSystemObject&amp;quot;)

'Set ft = FSO.OpenTextFile(&amp;quot;c:\PPCustomer\CoH\PP\In\Group.csv&amp;quot;,2,True)
Set ft = FSO.OpenTextFile(myPath &amp;amp; myFile,2,True)

ft.WriteLine(&amp;quot;Original_Sort_Order,FileID,ACCOUNT_NUMBER,First_Name,Address_Line_1,Address_Line_2,Address_Line_3,Locality,State,Postcode,Country,Source,Dt_Sort_Type,Sort_Order,Dt_BP_Sort_Order,Dt_Barcode,Dt_BSP&amp;quot;)

'for all documents in group 0
For x = 1 to MyMeta.Job.Group(0).Count

        'add new CSV line
'        MyCSVLine = MyMeta.Job.Group(0).Document(x-1).FieldByName(&amp;quot;FileID&amp;quot;)  + &amp;quot;,&amp;quot; + _
'                    MyMeta.Job.Group(0).Document(x-1).FieldByName(&amp;quot;ACCOUNT_NUMBER&amp;quot;)

        MyCSVLine = Trim(x)  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_FileID&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_ACCOUNT_NUMBER&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_First_Name&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Address_Line_1&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Address_Line_2&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Address_Line_3&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Locality&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_State&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Postcode&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Country&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Source&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Dt_Sort_Type&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Sort_Order&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Dt_BP_Sort_Code&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Dt_Barcode&amp;quot;))  + &amp;quot;,&amp;quot; + _
                    Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_Dt_BSP&amp;quot;))
        ft.WriteLine(MyCSVLine)

Next

ft.Close
Set FSO = Nothing
Set MyMeta = Nothing

'write a file to act as a trigger for the email notification process
myPath = Trim(Watch.GetVariable(&amp;quot;global.baseDir&amp;quot;)) &amp;amp; &amp;quot;\2 Processing\&amp;quot;
myFile = &amp;quot;TRIGGER&amp;quot;
Set FSO = CreateObject(&amp;quot;Scripting.FileSystemObject&amp;quot;)
Set ft = FSO.OpenTextFile(myPath &amp;amp; myFile,2,True)
ft.WriteLine(&amp;quot;fhugeyhfruyheuworfyuoeyhruoeywharuiyhewarfiuryherfhybeuir&amp;quot;)
ft.Close
Set FSO = Nothing
