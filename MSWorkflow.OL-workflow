<Config>&lt;PWScript&gt;
&lt;ScriptConfig&gt;'this script creates a CSV file containing address information to be cleansed by an address cleansing application
Option Explicit
Dim MyMeta, x, MyCSVLine, MyFileHeadings, FSO, ft
Dim myPath, myFile

Dim Addr2, Loc, State, Postcode, Country

Dim SeqNumber

Set MyMeta = CreateObject(&amp;quot;MetaDataLib.MetaFile&amp;quot;)
MyMeta.LoadFromFile(Watch.GetMetadataFileName)

'set file path &amp;amp; name
myPath = Trim(Watch.GetVariable(&amp;quot;global.SphereBaseDir&amp;quot;)) &amp;amp; &amp;quot;\Data\&amp;quot;
myFile =  Trim(Watch.GetVariable(&amp;quot;newFileName&amp;quot;))
myFile = Replace(myFile,&amp;quot;.&amp;quot;,&amp;quot;&amp;quot;)
myFile = myFile &amp;amp; &amp;quot;_unsorted.txt&amp;quot;

'create CSV
Set FSO = CreateObject(&amp;quot;Scripting.FileSystemObject&amp;quot;)

'Set ft = FSO.OpenTextFile(&amp;quot;c:\PPCustomer\CoH\PP\In\Group.csv&amp;quot;,2,True)
Set ft = FSO.OpenTextFile(myPath &amp;amp; myFile,2,True)

MyFileHeadings = chr(34) + &amp;quot;SeqNumber&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;LetterType&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;FirstName&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;AddressLine1&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;Locality&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;State&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;Postcode&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;Country&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;AccountNumber&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;ArrearsDue&amp;quot; + chr(34) + vbTab + _
            chr(34) + &amp;quot;LetterDate&amp;quot; + chr(34)
ft.WriteLine(MyFileHeadings)

SeqNumber = 1
'for all documents in group 0
For x = 1 to MyMeta.Job.Group(0).Count

        'add new CSV line

        Addr2 = &amp;quot;&amp;quot;
        Loc = &amp;quot;&amp;quot;
        State = &amp;quot;&amp;quot;
        Postcode = &amp;quot;&amp;quot;
        Country = &amp;quot;&amp;quot;

        Addr2 = Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_AddressLine2&amp;quot;))

        'skip empty pages
        if(Addr2 &amp;lt;&amp;gt; &amp;quot;&amp;quot;) then
          Postcode = trim(Right(Addr2,4))
          State = trim(Mid(Addr2,Len(Addr2)-7,3))
          Loc = trim(Left(Addr2,Len(Addr2)-8))

          if(Postcode = &amp;quot;0000&amp;quot;) then
                  Country = Loc
                  Loc = &amp;quot;&amp;quot;
                  State = &amp;quot;&amp;quot;
                  Postcode = &amp;quot;&amp;quot;
          end if

          MyCSVLine = chr(34) + Trim(SeqNumber) + chr(34) + vbTab + _
                      chr(34) + &amp;quot;PL_Letter&amp;quot; + chr(34) + vbTab + _
                      chr(34) + Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_FirstName&amp;quot;)) + chr(34) + vbTab + _
                      chr(34) + Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_AddressLine1&amp;quot;)) + chr(34) + vbTab + _
                      chr(34) + Loc + chr(34) + vbTab + _
                      chr(34) + State + chr(34) + vbTab + _
                      chr(34) + Postcode + chr(34) + vbTab + _
                      chr(34) + Country + chr(34) + vbTab + _
                      chr(34) + Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_LoanNumber&amp;quot;)) + chr(34) + vbTab + _
                      chr(34) + Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_ArrearsDue&amp;quot;)) + chr(34) + vbTab + _
                      chr(34) + Trim(MyMeta.Job().Group(0).Document(x-1).FieldByName(&amp;quot;_vger_fld_LetterDate&amp;quot;)) + chr(34)
          ft.WriteLine(MyCSVLine)
          SeqNumber = SeqNumber + 1
        end if
Next

ft.Close
Set FSO = Nothing
Set MyMeta = Nothing

'write a file to act as a trigger for the email notification process
myPath = Trim(Watch.GetVariable(&amp;quot;global.SphereBaseDir&amp;quot;)) &amp;amp; &amp;quot;\Data\&amp;quot;
myFile = &amp;quot;TRIGGER&amp;quot;
Set FSO = CreateObject(&amp;quot;Scripting.FileSystemObject&amp;quot;)
Set ft = FSO.OpenTextFile(myPath &amp;amp; myFile,2,True)
ft.WriteLine(&amp;quot;fhugeyhfruyheuworfyuoeyhruoeywharuiyhewarfiuryherfhybeuir&amp;quot;)
ft.Close
Set FSO = Nothing
